{
  "manifest_version": 2,
  "name": "IMDbleep",
  "version": "0.1",
  "description": "Hide potential spoilers from your new favourite tv shows with this simple plugin",
  "content_scripts": [
    {
      "matches": ["https://www.imdb.com/*"],
      "css": ["index.css"],
      "js": ["index.js"]
    }
  ],
  "icons": { "48": "media/icon48.png", "128": "media/icon128.png" }
}
